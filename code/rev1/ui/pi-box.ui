<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.94.1 -->
<interface>
  <!-- interface-name pi-box.ui -->
  <requires lib="gtk" version="4.8"/>
  <object class="GtkWindow" id="win">
    <property name="fullscreened">True</property>
    <child>
      <object class="GtkGrid">
        <child>
          <object class="GtkGrid">
            <child>
              <object class="GtkButton" id="cycle_f">
                <property name="label">Cycle Relays Fast</property>
                <signal name="clicked" handler="cycle_fast_relay"/>
                <layout>
                  <property name="column">1</property>
                  <property name="column-span">1</property>
                  <property name="row">1</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="cycle_s">
                <property name="label">Cycle Relays Slow</property>
                <signal name="clicked" handler="cycle_slow_relay"/>
                <layout>
                  <property name="column">2</property>
                  <property name="column-span">1</property>
                  <property name="row">1</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="cycle_t">
                <property name="label">Cycle Thu Relays</property>
                <signal name="clicked" handler="cycle_thru_relays"/>
                <layout>
                  <property name="column">1</property>
                  <property name="column-span">1</property>
                  <property name="row">2</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <layout>
              <property name="column">0</property>
              <property name="column-span">1</property>
              <property name="row">0</property>
              <property name="row-span">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkGrid">
            <child>
              <object class="GtkButton" id="quit">
                <property name="label">Quit</property>
                <property name="name">quit</property>
                <signal name="clicked" handler="quit"/>
                <layout>
                  <property name="column">2</property>
                  <property name="column-span">1</property>
                  <property name="row">2</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <layout>
              <property name="column">2</property>
              <property name="column-span">1</property>
              <property name="row">2</property>
              <property name="row-span">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkGrid">
            <child>
              <object class="GtkButton">
                <property name="label">Cycle Leds Fast</property>
                <signal name="clicked" handler="cycle_fast_led"/>
                <layout>
                  <property name="column">1</property>
                  <property name="column-span">1</property>
                  <property name="row">1</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton">
                <property name="label">Cycle Leds Slow</property>
                <signal name="clicked" handler="cycle_slow_led"/>
                <layout>
                  <property name="column">2</property>
                  <property name="column-span">1</property>
                  <property name="row">1</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton">
                <property name="label">Cycle Thru Leds</property>
                <signal name="clicked" handler="cycle_thru_leds"/>
                <layout>
                  <property name="column">1</property>
                  <property name="column-span">1</property>
                  <property name="row">2</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <layout>
              <property name="column">0</property>
              <property name="column-span">1</property>
              <property name="row">1</property>
              <property name="row-span">1</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
